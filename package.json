{
    "name": "wpkiosk",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . WPKiosk --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"WP Kiosk\"",
        "package-linux": "electron-packager . WPKiosk --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
        "build": "concurrently \"webpack\" \"gulp\" \"npm start\""
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "Anthony S. Talarico",
    "license": "private",
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@blueprintjs/core": "^3.15.1",
        "@material-ui/core": "^4.1.1",
        "@webpack-cli/add": "^0.2.2",
        "animate.css": "^3.7.2",
        "babel-core": "^7.0.0-0",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
        "babel-plugin-transform-react-router-optimize": "^1.0.2",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "compression-webpack-plugin": "^3.0.0",
        "concurrently": "^4.1.0",
        "css-loader": "^3.0.0",
        "dragula": "^3.7.2",
        "electron": "^5.0.4",
        "electron-packager": "^13.1.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "gulp": "^3.9.1",
        "gulp-concat": "^2.6.1",
        "gulp-copy": "^4.0.1",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-uglifyes": "^0.2.1",
        "jquery": "^3.4.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "^0.13.2",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.1.3",
        "webpack": "^4.34.0",
        "webpack-cli": "^3.3.4",
        "webpack-merge": "^4.2.1",
        "webpack-node-externals": "^1.7.2",
        "webpack-target-electron-renderer": "^0.4.0"
    }
}
